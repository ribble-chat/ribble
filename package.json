  "main": "src/electron.js",
  "dependencies": {
    "@microsoft/signalr": "^5.0.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-relay": "^7.0.17",
    "@types/react-router-dom": "^5.1.7",
    "assert": "^2.0.0",
    "axios": "^0.21.1",
    "graphql-ws": "^4.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-modal": "^3.12.1",
    "react-password-strength-bar": "^0.3.2",
    "react-relay": "^0.0.0-experimental-4c4107dd",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-spinners": "^0.10.6",
    "react-toastify": "^7.0.3",
    "recoil": "^0.1.2",
    "relay-hooks": "^4.1.1",
    "relay-runtime": "^10.1.3",
    "sass": "^1.32.6",
    "typescript": "^4.2.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "concurrently \"BROWSER=none yarn start-react\" \"wait-on http://localhost:3000 && electron .\"",
    "start-react": "concurrently \"yarn run relay --watch\" \"react-scripts start\"",
    "build": "yarn run relay && react-scripts build",
    "relay": "yarn run relay-compiler --schema schema.graphql --src ./src/ --language typescript $@",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-modal": "^3.12.0",
    "@types/relay-runtime": "^10.1.9",
    "babel-plugin-relay": "^10.1.3",
    "concurrently": "^5.3.0",
    "electron": "^11.2.3",
    "electron-is-dev": "^1.2.0",
    "graphql": "^15.5.0",
    "relay-compiler": "^10.1.3",
    "relay-compiler-language-typescript": "^13.0.6",
    "wait-on": "^5.2.1"
  }
}
